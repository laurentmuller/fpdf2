parameters:
    level: max
    tmpDir: cache/phpstan
    paths:
        - src
        - tests

    excludePaths:
        - tests/fonts
        - tests/Legacy
        - tests/Legacy/font
        - tests/resources
        - vendor

    type_coverage:
        constant: 100
        declare: 100
        param: 100
        property: 100
        return: 100

    reportUnmatchedIgnoredErrors: true
    checkUninitializedProperties: true

    ignoreErrors:
        -
             identifier: staticMethod.alreadyNarrowedType
             path: tests/
