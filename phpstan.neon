parameters:
    level: 10
    paths:
        - src
        - tests

    excludePaths:
        - src/font
        - tests/fonts
        - tests/FPDF.php
        - tests/resources/*.php
        - vendor

    reportUnmatchedIgnoredErrors: true
    checkUninitializedProperties: true

    ignoreErrors:
        -
             identifier: staticMethod.alreadyNarrowedType
             path: tests/
